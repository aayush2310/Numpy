#creating numpy/n-d arrays

import numpy as np

arr1=np.array([1,2,3,4,5,6])
arr1

type(arr1)

arr2=np.array([[1,2,3],[4,5,6]])
arr2

arr3=np.zeros((2,3))
arr3

arr4=np.ones((2,3))
arr4

arr5=np.identity(5)
arr5

arr6=np.arange(10)
arr6

arr6=np.arange(5,16,2)
arr6

arr7=np.linspace(10,20,10)
arr7

arr8=arr7.copy()
arr8
